{
    "SM.DEL": {
        "summary": "Deletes a state machine",
        "since": "0.1.0",
        "complexity": "O(N)",
        "group": "statemachine",
        "arguments": [
            {
                "name": "key",
                "type": "key"
            }
        ]
    },
    "SM.GET": {
        "summary": "Gets a state machine, or the current state of a machine",
        "since": "0.1.0",
        "complexity": "O(N)",
        "group": "statemachine",
        "arguments": [
            {
                "name": "key",
                "type": "key"
            },
            {
                "name": "state",
                "type": "string"
            }
        ]
    },
    "SM.SET": {
        "summary": "Imports a state machine from a json blob",
        "since": "0.1.0",
        "complexity": "O(N)",
        "group": "statemachine",
        "arguments": [
            {
                "name": "key",
                "type": "key"
            },
            {
                "name": "value",
                "type": "string"
            }
        ]
    },
    "SM.STATES": {
        "summary": "Gets the current state associated with a state machine",
        "since": "0.1.0",
        "complexity": "O(N)",
        "group": "statemachine",
        "arguments": [
            {
                "name": "key",
                "type": "key"
            }
        ]
    },
    "SM.RESET": {
        "summary": "Resets a state machine to its initial state",
        "since": "0.1.0",
        "complexity": "O(N)",
        "group": "statemachine",
        "arguments": [
            {
                "name": "key",
                "type": "key"
            }
        ]
    },
    "SM.CURRENT": {
        "summary": "Retrieves the current state machine state",
        "since": "0.1.0",
        "complexity": "O(N)",
        "group": "statemachine",
        "arguments": [
            {
                "name": "key",
                "type": "key"
            }
        ]
    },
    "SM.CREATE": {
        "summary": "Creates a blank state machine and stores it in the named key",
        "since": "0.1.0",
        "complexity": "O(N)",
        "group": "statemachine",
        "arguments": [
            {
                "name": "key",
                "type": "key"
            }
        ]
    },
    "SM.TEMPLATE": {
        "summary": "Return the json template used for constructing a state machine",
        "since": "0.1.0",
        "complexity": "O(1)",
        "group": "statemachine"
    },
    "SM.TRANSITION": {
        "summary": "Transition the state machine to the specific state",
        "since": "0.1.0",
        "complexity": "O(1)",
        "group": "statemachine",
        "arguments": [
            {
                "name": "key",
                "type": "key"
            },
            {
                "name": "state",
                "type": "string"
            },
            {
                "name": "codition",
                "type": "string",
                "optional": true,
                "arguments": [
                    {
                        "name": "force",
                        "type": "pure-token",
                        "token": "F"
                    }
                ]
            }
        ]
    }
}